@model ccct2019.Data.NganchanGiaitoa
<div class="modal fade m-auto" id="XoancgtModal" aria-hidden="true" style="display: none; width:20% !important; margin-top:10% !important; margin-left:40% !important;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title">Bạn có muốn xóa thông tin này không?</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>

            <div class="modal-body justify-content-center text-center">
                <input type="hidden" id="User" value="@Model.UserID" />
                <div class="row">
                    <div class="col">
                        <button type="button" class="btn btn-success btn-block" onclick="btnDeletencgt(@Model.ID);">Có</button>
                    </div>
                    <div class="col">
                        <button type="button" class="btn btn-outline-dark btn-block" data-dismiss="modal">Không</button>
                    </div>
                </div>


            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
